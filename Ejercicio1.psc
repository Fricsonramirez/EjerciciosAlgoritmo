Algoritmo Edad_altura
	//Definr variables 
	
	Definir edad Como Entero;
	Definir altura Como Real;
	
	//Datos de entrada
	
	Escribir 'Ingresa tu edad';
	Leer edad;
	Escribir 'Ingresa tu altura';
	Leer altura;
	
	//Datos de salida
	
	Escribir 'Tienes ",edad," años';
	Escribir 'Pesas ",altura," kilos ';
FinAlgoritmo
